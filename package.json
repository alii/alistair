{
	"name": "alistair",
	"version": "1.14.1",
	"repository": "https://github.com/alii/alistair",
	"author": "Alistair Smith <hi@alistair.sh>",
	"license": "MIT",
	"sideEffects": false,
	"type": "module",
	"scripts": {
		"build": "tsup",
		"release": "yarn build && yarn npm publish"
	},
	"types": "./dist/index.d.cts",
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"exports": {
		"./package.json": "./package.json",
		"./prettier": "./prettier/index.json",
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		},
		"./id": {
			"import": "./dist/id/index.js",
			"require": "./dist/id/index.cjs"
		},
		"./http": {
			"import": "./dist/http/index.js",
			"require": "./dist/http/index.cjs"
		},
		"./hooks": {
			"import": "./dist/hooks/index.js",
			"require": "./dist/hooks/index.cjs"
		},
		"./react": {
			"import": "./dist/react/index.js",
			"require": "./dist/react/index.cjs"
		},
		"./bus": {
			"import": "./dist/bus/index.js",
			"require": "./dist/bus/index.cjs"
		},
		"./atoms": {
			"import": "./dist/atoms/index.js",
			"require": "./dist/atoms/index.cjs"
		},
		"./structs": {
			"import": "./dist/structs/index.js",
			"require": "./dist/structs/index.cjs"
		},
		"./tags": {
			"import": "./dist/tags/index.js",
			"require": "./dist/tags/index.cjs"
		},
		"./log": {
			"import": "./dist/log/index.js",
			"require": "./dist/log/index.cjs"
		}
	},
	"files": [
		"README.md",
		"LICENSE",
		"dist",
		"prettier",
		"package.json"
	],
	"devDependencies": {
		"@types/react": "^19.0.7",
		"tsup": "^8.3.5",
		"typescript": "^5.7.3"
	},
	"peerDependencies": {
		"react": "*"
	},
	"keywords": [
		"utilities",
		"react",
		"hooks",
		"focused",
		"localstorage",
		"prettier",
		"formatting",
		"id"
	],
	"publishConfig": {
		"access": "public"
	},
	"packageManager": "yarn@4.5.0",
	"dependencies": {
		"@types/node": "^22.10.10",
		"colorette": "^2.0.20"
	}
}
